pipeline{
		
		agent {
			label{
				label "built-in"
				customWorkspace "/mnt/git_new"
			
			}
		
		
		
		}
		
		stages {
		
			stage('stage-1'){
			
			steps{
			  echo "this is stage-1"
			
			
			}
			}
			stage('stage-2') {
			
			steps {
					echo "this is stage-2"
			
			}
			
			
			}
			stage('stage-3') {
			
			steps {
					echo "this is stage 3"
			
			}
			
			}
			 
				
			
			
		}

}
